/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 0 auto;
  line-height: 60rpx;
  font-size: 28rpx;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
.color_1{
  color: #E70012;
}

.run{
	animation:myfirst 1s;
	-webkit-animation:myfirst 1s; /* Safari and Chrome */
  position: relative;
  top: 0;
  right: 0;
}

@keyframes myfirst
{
	from {
    position: relative;
    top: 0;
    right: 0;
    height: 570rpx;
  }
	to {
    position: relative;
    top: 0;
    right: 750rpx;
    height: 0;
  }
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
	from {
    position: relative;
    top: 0;
    right: 0;
    height: 222rpx;
  }
	to {
    position: relative;
    top: 0;
    right: 750rpx;
    height: 0;
  }
}